{
  "name": "uploadthing-turbo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "build:pkgs": "turbo run build --filter \"./packages/*\"",
    "clean": "turbo run clean && git clean -xdf node_modules",
    "dev": "turbo run dev",
    "dev:pkgs": "turbo run dev --filter \"./packages/*\"",
    "lint": "turbo run lint && manypkg check",
    "lint:fix": "turbo run lint -- --fix && manypkg check",
    "format": "prettier --write .",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "typecheck": "turbo run typecheck"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@manypkg/cli": "^0.20.0",
    "@types/node": "18.16.0",
    "@uploadthing/eslint-config": "0.1.0",
    "dotenv": "latest",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "turbo": "^1.10.1",
    "typescript": "5.1.3"
  },
  "packageManager": "pnpm@8.6.0"
}
